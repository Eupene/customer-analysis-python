      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "\n",
        "data = {\n",
        "    \"CustomerID\": [101, 102, 103, 104, 105],\n",
        "    \"Age\": [25, 34, 28, 40, 23],\n",
        "    \"Gender\": [\"Female\", \"Male\", \"Female\", \"Male\", \"Female\"],\n",
        "    \"City\": [\"Accra\", \"Kumasi\", \"Accra\", \"Tamale\", \"Kumasi\"],\n",
        "    \"ProductPurchased\": [\"Laptop\", \"Smartphone\", \"Tablet\", \"Smartwatch\", \"Laptop\"],\n",
        "    \"PurchaseAmount\": [1200, 800, 400, 250, 1100],\n",
        "    \"PurchaseDate\": [\"2024-08-01\", \"2024-08-03\", \"2024-08-04\", \"2024-08-05\", \"2024-08-07\"],\n",
        "    \"CustomerSatisfaction\": [8, 9, 7, 6, 9],\n",
        "}\n",
        "\n",
        "df = pd.DataFrame(data)\n",
        "\n",
        "# Group by product and calculate average age\n",
        "product_age = df.groupby('ProductPurchased')['Age'].mean()\n",
        "print(\"Average age of customers by product:\\n\", product_age)\n",
        "\n",
        "# Group by product and count gender distribution\n",
        "product_gender = df.groupby(['ProductPurchased', 'Gender'])['CustomerID'].count().unstack(fill_value=0)\n",
        "print(\"\\nGender distribution by product:\\n\", product_gender)\n",
        "\n",
        "# You can further visualize these results using libraries like matplotlib or seaborn\n",
        "# For example, to create a bar chart of gender distribution by product:\n",
        "import matplotlib.pyplot as plt\n",
        "product_gender.plot(kind='bar', stacked=True)\n",
        "plt.title('Gender Distribution by Product Purchased')\n",
        "plt.xlabel('Product')\n",
        "plt.ylabel('Number of Customers')\n",
        "plt.show()\n",
        "\n",
        "# Calculate the correlation coefficient\n",
        "correlation = df['PurchaseAmount'].corr(df['CustomerSatisfaction'])\n",
        "print(\"Correlation coefficient between Purchase Amount and Customer Satisfaction:\", correlation)\n",
        "\n",
        "# Visualize the relationship using a scatter plot\n",
        "plt.figure(figsize=(8, 6))\n",
        "plt.scatter(df['PurchaseAmount'], df['CustomerSatisfaction'])\n",
        "plt.title('Relationship between Purchase Amount and Customer Satisfaction')\n",
        "plt.xlabel('Purchase Amount')\n",
        "plt.ylabel('Customer Satisfaction')\n",
        "plt.grid(True)\n",
        "plt.show()\n",
        "\n",
        "# Convert PurchaseDate to datetime objects\n",
        "df['PurchaseDate'] = pd.to_datetime(df['PurchaseDate'])\n",
        "\n",
        "# Extract the day of the week (0 = Monday, 6 = Sunday)\n",
        "df['DayOfWeek'] = df['PurchaseDate'].dt.dayofweek\n",
        "\n",
        "# Count purchases for each day of the week\n",
        "daily_purchases = df['DayOfWeek'].value_counts().sort_index()\n",
        "\n",
        "# Map day numbers to day names for better readability\n",
        "day_names = ['Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday', 'Sunday']\n",
        "# Create a mapping of day numbers to day names\n",
        "day_mapping = {i: day for i, day in enumerate(day_names)}\n",
        "# Use the mapping to rename the index\n",
        "daily_purchases.index = daily_purchases.index.map(day_mapping)\n",
        "\n",
        "# Visualize the trend using a bar chart\n",
        "plt.figure(figsize=(10, 6))\n",
        "daily_purchases.plot(kind='bar')\n",
        "plt.title('Purchasing Trends by Day of the Week')\n",
        "plt.xlabel('Day of the Week')\n",
        "plt.ylabel('Number of Purchases')\n",
        "plt.grid(True)\n",
        "plt.show()\n",
        "\n",
        "print(daily_purchases)"
      ],
